package model;

import java.time.LocalDate;

public class BookHistory {
	Book b= new Book();
	LocalDate i,r;
	
	public BookHistory(){
		
	}
	
	public BookHistory(Book b,LocalDate i,LocalDate r)
	{
		this.b=b;
		this.i=i;
		this.r=r;
	}
	
	

	public Book getB() {
		return b;
	}

	public void setB(Book b) {
		this.b = b;
	}

	public LocalDate getI() {
		return i;
	}

	public void setI(LocalDate i) {
		this.i = i;
	}

	public LocalDate getR() {
		return r;
	}

	public void setR(LocalDate r) {
		this.r = r;
	}

	@Override
	public String toString() {
		return "BookHistory [book=" + b.toString() + ", issuedate=" + i + ", returndate=" + r + "]";
	}
	
	
}
